{
  "name": "@tago-leap/shared",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "main": "./dist/types/index.js",
  "module": "./dist/types/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "default": "./dist/types/index.js"
    },
    "./env": {
      "types": "./dist/env/index.d.ts",
      "import": "./dist/env/index.js",
      "default": "./dist/env/index.js"
    },
    "./supabase": {
      "types": "./dist/supabase/index.d.ts",
      "import": "./dist/supabase/index.js",
      "default": "./dist/supabase/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "default": "./dist/types/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "types": ["./dist/types/index.d.ts"],
      "env": ["./dist/env/index.d.ts"],
      "supabase": ["./dist/supabase/index.d.ts"]
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}
