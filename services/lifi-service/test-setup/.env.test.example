# LIFI Service Test Environment
# Copy this to ../../.env or set these in your environment

# Supabase (required - use your existing Supabase project)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Service Ports
LIFI_SERVICE_PORT=3002
MOCK_SALT_PORT=3003

# LI.FI API (defaults work - no auth needed)
LIFI_API_BASE_URL=https://li.quest/v1
LIFI_INTEGRATOR=tago-leap

# Hyperliquid (defaults work)
HYPERLIQUID_API_BASE_URL=https://api.hyperliquid.xyz
HYPEREVM_CHAIN_ID=999

# Salt Service URL (points to mock)
SALT_SERVICE_URL=http://localhost:3003
